This is a placeholder for the service description!

<% form_tag :action => 'login' do %>
<table class="loginForm" ><tbody>
<tr>
<label for="user_email">Email:</label><br/>
  <%= text_field "user", "email", :size => 20 %><br/>
</tr>
<tr>  <label for="user_password">Password:</label><br/>
  <%= password_field "user", "password", :size => 20 %><br/>
</tr>
<tr>
<td><%= submit_tag "Login" %></td>
</tr>
</tbody></table>
<%= link_to 'Signup now', signup_user_path %>

<% end %>
